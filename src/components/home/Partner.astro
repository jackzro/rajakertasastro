---
import { getPartner } from "@/helpers/rajaKertas"
import Mitra from './Mitra'

const response = await getPartner()

---

<section class="flex flex-col justify-center items-center space-y-6 p-20 text-black bg-white">
    <div class="flex justify-center items-center w-full mb-10">
        <h1 class=" text-2xl md:text-3xl lg:text-5xl font-extrabold ">Partner Kami</h1>
    </div>
    
    <Mitra client:load data={response}/>
</section>